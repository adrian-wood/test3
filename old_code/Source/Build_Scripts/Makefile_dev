C_OBJS = MetDB_c_utils.o \
         mdbwrap.o \
         dynalc.o \
         system_cmd.o

BUFRC_OBJS = MetDB_c_utils.o \
             bufrwrap.o


#-----------------------------------------------------------------------
# Rule to run builds
#-----------------------------------------------------------------------
ALL: $(BUFRDAT) $(STOREUM) $(MDBSTOR) $(BUFRDLL) $(WEBRET) \
     $(WEBTEST) $(STATSOBS) $(HOWFULL) $(UKSYNPCT) $(STORAMD)


#-----------------------------------------------------------------------
# Rule to build MDBDLL
#-----------------------------------------------------------------------

$(DLL): ${MDB_OBJS} ${C_OBJS}
	c89 -Wl,dll \
        -o ${DLL} ${MDB_OBJS} ${C_OBJS} \
        ${STND_CODE}/zpdate_mod.o \
        ${STND_CODE}/int2ch_mod.o \
        ${STND_CODE}/stnmas_mod.o \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a
	cp -X ${DLL} ${LOAD_LIB}

#-----------------------------------------------------------------------
# Rule to build MDBSTOR
#-----------------------------------------------------------------------

$(MDBSTOR): ${MDBSTOR_OBJS}
	c89 -o ${MDBSTOR} ${MDBSTOR_OBJS} \
        ${STND_CODE}/int2ch_mod.o \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a
	@echo "**********************************************************************"
	@echo "   Expect messages like 'SYMBOL sysabn_ UNRESOLVED'! "
	@echo "Complete the test build using a copy of MDB.TESTJOBS(MSTORBD) "
	@echo "Complete the operational build using MCC3.DBJCLINK.CNTL(MSTOR)"
	@echo "**********************************************************************"

#-----------------------------------------------------------------------
# Rule to build STORAMD
#-----------------------------------------------------------------------

$(STORAMD): ${STORAMD_OBJS}
	c89 -o ${STORAMD} ${STORAMD_OBJS} \
        ${STND_CODE}/int2ch_mod.o \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a
	@echo "**********************************************************************"
	@echo "   Expect messages like 'SYMBOL sysabn_ UNRESOLVED'! "
	@echo "Complete the test build using a copy of MDB.TESTJOBS(STAMDBD) "
	@echo "Complete the oper build using MCC3.DBJCLINK.CNTL(STORAMD)"
	@echo "**********************************************************************"

#-----------------------------------------------------------------------
# Rule to build MONITOR
#-----------------------------------------------------------------------

$(MONITOR): ${MONITOR_OBJS}
	c89 -o ${MONITOR} ${MONITOR_OBJS} \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a
	@echo "**********************************************************************"
	@echo "   Expect messages like 'SYMBOL sysabn_ UNRESOLVED'! "
	@echo "Complete the test build using a copy of MDB.TESTJOBS(MONTORBD)"
	@echo "Complete the oper build using MCC3.DBJCLINK.CNTL(MONITOR)"
	@echo "**********************************************************************"

#-----------------------------------------------------------------------
# Rule to build BUFRDAT
#-----------------------------------------------------------------------

$(BUFRDAT): ${BUFRDAT_OBJS}
	c89 -o ${BUFRDAT} ${BUFRDAT_OBJS} \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a
	@echo "**********************************************************************"
	@echo "   Expect messages like 'SYMBOL sysabn_ UNRESOLVED'! "
	@echo "Complete the test build using a copy of MDB.TESTJOBS(BFDATBD)"
	@echo "Complete the oper build using MCC3.DBJCLINK.CNTL(BUFRDAT)"
	@echo "**********************************************************************"

#-----------------------------------------------------------------------
# Rule to build HOWFULL
#-----------------------------------------------------------------------

$(HOWFULL): ${HOWFULL_OBJS}
	xlc -o ${HOWFULL} ${HOWFULL_OBJS} \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a
	cp -X ${HOWFULL} ${LOAD_LIB}

#-----------------------------------------------------------------------
# Rule to build STATSOBS
#-----------------------------------------------------------------------

$(STATSOBS): ${STATSOBS_OBJS}
	xlc -o ${STATSOBS} ${STATSOBS_OBJS} \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a
	cp -X ${STATSOBS} ${LOAD_LIB}

#-----------------------------------------------------------------------
# Rule to build STOREUM
#-----------------------------------------------------------------------

$(STOREUM): ${STOREUM_OBJS}
	c89 -o ${STOREUM} ${STOREUM_OBJS} \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a
	@echo "**********************************************************************"
	@echo "   Expect messages like 'SYMBOL sysabn_ UNRESOLVED'! "
	@echo "Complete the test build using a copy of MDB.TESTJOBS(STEUMBD)"
	@echo "Complete the oper build using MCC3.DBJCLINK.CNTL(STOREUM)"
	@echo "**********************************************************************"

#-----------------------------------------------------------------------
# Rule to build BUFRDLL
#-----------------------------------------------------------------------

$(BUFRDLL): ${BUFR_OBJS} ${BUFRC_OBJS} ${STND_CODE}/int2ch_mod.o
	c89 -Wl,dll \
        -o ${BUFRDLL} ${BUFR_OBJS} ${BUFRC_OBJS} \
        ${STND_CODE}/int2ch_mod.o \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a
	cp -X ${BUFRDLL} ${LOAD_LIB}

#-----------------------------------------------------------------------
# Rule to build MERGE
#-----------------------------------------------------------------------

$(MERGE): ${MERGE_OBJS} ${BUFR_OBJS}
	c89 -o ${MERGE} -Wc,dll ${MERGE_OBJS} ${BUFR_OBJS} \
        MDBDLL.x \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a
	@echo "**********************************************************************"
	@echo "No test build required for intergration testing"
	@echo "Complete the oper build using MCC3.DBJCLINK.CNTL(MERGE)"
	@echo "**********************************************************************"

#-----------------------------------------------------------------------
# Rule to build WEBRET
#-----------------------------------------------------------------------

$(WEBRET): ${WEB_RET_OBJS}
	c89 -Wl,dll -o webret.exe ${WEB_RET_OBJS} \
        ${STND_CODE}/int2ch_mod.o \
        MDBDLL.x \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a
	@echo "**********************************************************************"
	@echo "       Copy executable (webret.exe) to running folder "
	@echo "**********************************************************************"

#-----------------------------------------------------------------------
# Rule to build WEBTEST
#-----------------------------------------------------------------------

$(WEBTEST): ${WEB_TEST_OBJS}
	c89 -Wl,dll -o webtst.exe ${WEB_TEST_OBJS} \
        ${STND_CODE}/int2ch_mod.o \
        MDBDLL.x \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a
	@echo "**********************************************************************"
	@echo "       Copy executable (webtst.exe) to running folder "
	@echo "**********************************************************************"

#-----------------------------------------------------------------------
# Rule to build UKSYNPCT
#-----------------------------------------------------------------------

$(UKSYNPCT): ${UKSYNPCT_OBJS}
	c89 -Wl,dll -o uksynpct.exe ${UKSYNPCT_OBJS} \
         MDBDLL.x \
        ${STND_CODE}/zpdate_mod.o \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a
	@echo "**********************************************************************"
	@echo "       Copy executable (uksynpct.exe) to running folder "
	@echo "**********************************************************************"

#-----------------------------------------------------------------------
# Rule to build SALTSSH
#-----------------------------------------------------------------------

$(SALTSSH): ${SALTSSH_OBJS}
	c89 -Wl,dll \
	    -o ${SALTSSH} ${SALTSSH_OBJS} \
        ${OLD_CODE}/BUFRDLL.x \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a
	@echo "**********************************************************************"
	@echo "   Expect messages  'A711 EXTERNAL SYMBOL ides_...' "
	@echo "                    'A712 THE ORIGINAL DEFINITIO...' "
	@echo "                    '9207 SYMBOL mhsiffc_ UNRESOLVED.' "
	@echo "                    '9207 SYMBOL sysabn_ UNRESOLVED.' "
	@echo "Complete the operational build using MCC3.DBJCLINK.CNTL(SALTSSH)"
	@echo "**********************************************************************"

#-----------------------------------------------------------------------
# Rules to build object code
#-----------------------------------------------------------------------

#
# .c source code to .o
#
# -DF95 because this version will be called from f95 code in MDBDLL
bufrshel95.o: ${OLD_CODE}/bufrshel.c
	${CC} -o bufrshel95.o ${CFLAGS} -DF95 -Wc,dll -c ${OLD_CODE}/bufrshel.c

bufrwrap.o: ${OLD_CODE}/bufrwrap.c
	${CC} ${CFLAGS} -Wc,"EXPORTALL" -c ${OLD_CODE}/bufrwrap.c

dynalc.o: ${OLD_CODE}/dynalc.c
	${CC} ${CFLAGS} -c ${OLD_CODE}/dynalc.c

getid.o: ${OLD_CODE}/getid.c
	${CC} ${CFLAGS} -c ${OLD_CODE}/getid.c

mdbc.o: ${OLD_CODE}/mdbc.c
	${CC} ${CFLAGS} -c -DF95 -Wc,dll ${OLD_CODE}/mdbc.c

mdbwrap.o: ${OLD_CODE}/mdbwrap.c
	${CC} ${CFLAGS} -Wc,"EXPORTALL" -c ${OLD_CODE}/mdbwrap.c

MetDB_c_utils.o: ${OLD_CODE}/MetDB_c_utils.c
	xlc -DUNDERSCORE -c ${OLD_CODE}/MetDB_c_utils.c

secsout.o: ${OLD_CODE}/secsout.c
	${CC} ${CFLAGS} -c ${OLD_CODE}/secsout.c

system_cmd.o: ${OLD_CODE}/system_cmd.c
	${CC} ${CFLAGS} -c ${OLD_CODE}/system_cmd.c


#
# .F and .f source to .o
#
