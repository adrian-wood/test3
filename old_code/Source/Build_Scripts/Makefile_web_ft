#-----------------------------------------------------------------------
#
# FILE NAME      : Makefile
#
# TYPE           : Makefile
#
# PURPOSE        : Builds WEBRET, WEBTEST & WEBUPDATE in the working
#                  directory
#
# NOTE 1         : zpdate_mod.o has to copied into main folder INCL_COD
#                  from /usr/local/mdbtest/test/
#
# NOTE 2         : If there are compilation problems with int2ch then
#                  delete int2ch_mod.mod from mods folder. int2ch_mod.o
#                  is created by int2ch_mod.mod rule.
#
# NOTE 3         : The module file _mod files are created with lower
#                  case names. It has been found it is better to retain
#                  to retain those lower case names for usage in f90
#                  programs.
#
# REVISION INFO  :
#
# $Source:$
# $Date: 09/06/2011 15:14:16$
# $Revision: 7$
#
# $Author: John Norton$
#
# $Log:
#  7    MetDB_Refresh 1.6         09/06/2011 15:14:16    John Norton
#       Updated location of MDBDLL.x and executable file names.
#  6    MetDB_Refresh 1.5         09/06/2011 15:08:03    Richard Weedon  build
#       dirs updated for ukmet
#  5    MetDB_Refresh 1.4         09/06/2011 15:01:54    Richard Weedon  
#  4    MetDB_Refresh 1.3         09/06/2011 12:59:06    John Norton     
#  3    MetDB_Refresh 1.2         09/06/2011 11:34:14    John Norton     
#  2    MetDB_Refresh 1.1         09/06/2011 10:28:05    John Norton
#       Removed use of MDBPRE.f and updated build options for mdb.c.
#  1    MetDB_Refresh 1.0         21/04/2011 11:58:43    John Norton     Ported
#        retrieval code
# $
#-----------------------------------------------------------------------
#-----------------------------------------------------------------------
# Define the compiler parameters for NAG f95 compiler
#-----------------------------------------------------------------------
# -DBPATH (Bufr library), -save (implicit SAVE), -g (debug flag),
# -DMVS (for code to run under MVS)
# -DEBCDIC (for code on GPCS)
# -C (range checking), -maxcontin (max continuation lines),
# -C=all (max compiler checks)
# -gline debug traceback
# -Wc,+z (needed to construct shared library)
#-----------------------------------------------------------------------

FF 	= f95
CC 	= cc

WEBRET = WEBRET
WEBTEST = WEBTEST
WEBUPDATE = WEBUPDATE

# DLLDEST = need changing whenever a new library for the executable to be sent to
DLLTEST = /usr/local/mdb/mdb_refresh/MDBDLL.x

FFLAGS = -C=all -gline
CFLAGS = -I/usr/local/lib/f95 -c

#-----------------------------------------------------------------------
#- WEBRET modules
#-----------------------------------------------------------------------

INCL_DIR  = /usr/local/mdb/mdb_refresh/mods

#-----------------------------------------------------------------------
#- Standard MetDB code - copied to my library!
#-----------------------------------------------------------------------

INCL_COD  = /usr/local/mdb/mdb_refresh

#-----------------------------------------------------------------------
#- Standard MetDB modules - copied to my library!
#-----------------------------------------------------------------------

INCL_MOD  = /usr/local/mdb/mdb_refresh/mods


#   Location of built exe file
EXE_DIR=/usr/local/mdb/mdb_refresh


IFLAGS 	= -I${INCL_DIR}

RET_OBJS = webair.o \
 webbuoyp.o \
 webdis.o \
 webesaw.o \
 webform.o \
 webform2.o \
 webgexp.o \
 weblnd.o \
 webncm.o \
 webret.o \
 websamos.o \
 webshp.o \
 websrew.o \
 webssea.o \
 webtadv.o \
 webtbus.o \
 webtfmet.o \
 webupr.o \
 code.o \
 delspce.o \
 ides.o \
 inquire.o \
 getchr.o \
 MetDB_c_utils.o \
 mdbc.o \
 readcf.o \
 value.o

TEST_OBJS = webair.o \
 webbuoyp.o \
 webdis.o \
 webesaw.o \
 webform.o \
 webform2.o \
 webgexp.o \
 weblnd.o \
 webncm.o \
 webretst.o \
 websamos.o \
 webshp.o \
 websrew.o \
 webssea.o \
 webtadv.o \
 webtbus.o \
 webtfmet.o \
 webupr.o \
 code.o \
 delspce.o \
 ides.o \
 inquire.o \
 getchr.o \
 MetDB_c_utils.o \
 mdbc.o \
 readcf.o \
 value.o

UPDATE_OBJS = WebRetFormMain_IBM.o \
 WebRetFormSelectAllVersions.o \
 WebRetFormSelectArea.o \
 WebRetFormSelectHeader.o \
 WebRetFormSelectIncrement.o \
 WebRetFormSelectMaxObs.o \
 WebRetFormSelectSondeParts.o \
 datim.o \
 zpdate_mod.o

#-----------------------------------------------------------------------
# Rule to run builds
#-----------------------------------------------------------------------
ALL: ${WEBRET} $(WEBTEST) $(WEBUPDATE)

#-----------------------------------------------------------------------
# Rule to build WEBRET
#-----------------------------------------------------------------------

$(WEBRET): ${RET_OBJS}
	c89 -Wl,"MAP" -o ${EXE_DIR}/webret.exe ${RET_OBJS} \
         int2ch_mod.o \
         MDBDLL.x \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a > link.out

#-----------------------------------------------------------------------
# Rule to build WEBTEST
#-----------------------------------------------------------------------

$(WEBTEST): ${TEST_OBJS}
	c89 -Wl,"MAP" -o ${EXE_DIR}/webtst.exe ${TEST_OBJS} \
         int2ch_mod.o \
         MDBDLL.x \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a > link.out

#-----------------------------------------------------------------------
# Rule to build WEBUPDATE
#-----------------------------------------------------------------------

$(WEBUPDATE): ${UPDATE_OBJS}
	c89 -Wl,"MAP" -o ${EXE_DIR}/webupd.exe ${UPDATE_OBJS} \
         int2ch_mod.o \
         MDBDLL.x \
        /usr/local/lib/f95/quickfit.o \
        /usr/local/lib/f95/libf96.a > link.out

#-----------------------------------------------------------------------
# Rules to build object code
#-----------------------------------------------------------------------


#
# .F and .f source to .o
#

webair.o: webair.f90 ${INCL_DIR}/webform_mod.mod
	${FF} ${IFLAGS} ${FFLAGS} -c webair.f90

webbuoyp.o: webbuoyp.f90 ${INCL_DIR}/code_mod.mod \
                         ${INCL_DIR}/ides_mod.mod \
                         ${INCL_DIR}/webform2_mod.mod
	${FF} ${IFLAGS} ${FFLAGS} -c webbuoyp.f90

webdis.o: webdis.f90 ${INCL_DIR}/code_mod.mod \
                     ${INCL_DIR}/getchr_mod.mod \
                     ${INCL_DIR}/ides_mod.mod
	${FF} ${IFLAGS} ${FFLAGS} -c webdis.f90

webesaw.o: webesaw.f90 ${INCL_DIR}/webform_mod.mod
	${FF} ${IFLAGS} ${FFLAGS} -c webesaw.f90

webform.o: webform.f90
	${FF} ${IFLAGS} ${FFLAGS} -c webform.f90

webform2.o: webform2.f90
	${FF} ${IFLAGS} ${FFLAGS} -c webform2.f90

webgexp.o: webgexp.f90
	${FF} ${IFLAGS} ${FFLAGS} -c webgexp.f90

weblnd.o: weblnd.f90 ${INCL_DIR}/webform_mod.mod \
                     ${INCL_DIR}/webgexp_mod.mod
	${FF} ${IFLAGS} ${FFLAGS} -c weblnd.f90

webncm.o: webncm.f90 ${INCL_DIR}/webform_mod.mod \
                     ${INCL_DIR}/webgexp_mod.mod
	${FF} ${IFLAGS} ${FFLAGS} -c webncm.f90

webret.o: webret.f90 ${INCL_DIR}/delspce_mod.mod \
