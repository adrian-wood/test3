webtadv.o: webtadv.f90
	${FF} ${IFLAGS} ${FFLAGS} -c webtadv.f90

webtbus.o: webtbus.f90
	${FF} ${IFLAGS} ${FFLAGS} -c webtbus.f90

webtfmet.o: webtfmet.f90 ${INCL_DIR}/webform_mod.mod
	${FF} ${IFLAGS} ${FFLAGS} -c webtfmet.f90

webupr.o: webupr.f90 ${INCL_DIR}/webform_mod.mod
	${FF} ${IFLAGS} ${FFLAGS} -c webupr.f90

code.o: code.F90 ${INCL_DIR}/readcf_mod.mod \
                 ${INCL_DIR}/inquire_mod.mod \
                 ${INCL_DIR}/int2ch_mod.mod \
                 ${INCL_DIR}/value_mod.mod \
                 ${INCL_DIR}/bufrpath_mod.mod
	${FF} ${IFLAGS} ${FFLAGS} -c code.F90

delspce.o: delspce.f90
	${FF} ${IFLAGS} ${FFLAGS} -c delspce.f90

getchr.o: getchr.f90
	${FF} ${IFLAGS} ${FFLAGS} -c getchr.f90

ides.o: ides.f90
	${FF} ${IFLAGS} ${FFLAGS} -c ides.f90

inquire.o: inquire.F90
	${FF} ${IFLAGS} ${FFLAGS} -c inquire.F90

mdbc.o: mdbc.c
	c89 -I/usr/local/lib/f95 -DF95 -c -Wc,dll mdbc.c

readcf.o: readcf.f90
	${FF} ${IFLAGS} ${FFLAGS} -c readcf.f90

value.o: value.f90
	${FF} ${IFLAGS} ${FFLAGS} -c value.f90

datim.o: datim.f90
	${FF} ${IFLAGS} ${FFLAGS} -c datim.f90

WebRetFormMain_IBM.o: WebRetFormMain_IBM.f90 \                          \
                 ${INCL_DIR}/webretformselectallversions_mod.mod \
                 ${INCL_DIR}/webretformselectarea_mod.mod \
                 ${INCL_DIR}/webretformselectheader_mod.mod \
                 ${INCL_DIR}/webretformselectincrement_mod.mod \
                 ${INCL_DIR}/webretformselectmaxobs_mod.mod \
                 ${INCL_DIR}/webretformselectsondeparts_mod.mod \
                 ${INCL_DIR}/datim_mod.mod \
                 ${INCL_DIR}/zpdate_mod.mod
	${FF} ${IFLAGS} ${FFLAGS} -c WebRetFormMain_IBM.f90

WebRetFormSelectAllVersions.o: WebRetFormSelectAllVersions.f90
	${FF} ${IFLAGS} ${FFLAGS} -c WebRetFormSelectAllVersions.f90

