/*
//*---------------------------------------------------------------------
//* Step 2:  Yesterday's statistics for TEMPS, PILOTS and DROPSONDES.
//*---------------------------------------------------------------------
//STEP2  EXEC PGM=STATSBUF
//STEPLIB  DD DSN=MDB.LOADLIB,DISP=SHR
//FT06F001 DD SYSOUT=Q
//FT21F001 DD DSN=TDB.STATS.TEMPF,DISP=OLD
//FT22F001 DD DSN=TDB.STATS.TEMPM,DISP=OLD
//FT23F001 DD DSN=TDB.STATS.PILOTF,DISP=OLD
//FT24F001 DD DSN=TDB.STATS.PILOTM,DISP=OLD
//FT25F001 DD DSN=TDB.STATS.DROPSOND,DISP=OLD
//FT51F001 DD DSN=TDB.UPRAIR,DISP=SHR
//FT52F001 DD DSN=TDB.UPRAIR,DISP=SHR
//FT53F001 DD DSN=TDB.UPRAIR,DISP=SHR
//FT54F001 DD DSN=TDB.UPRAIR,DISP=SHR
//FT55F001 DD DSN=TDB.UPRAIR,DISP=SHR
//*
//*---------------------------------------------------------------------
//* Step 3:  Yesterday's statistics for various non-BUFR data types.
//*---------------------------------------------------------------------
//STEP3  EXEC PGM=STATSMET,TIME=(4,00)
//STEPLIB  DD DSN=MDB.LOADLIB,DISP=SHR
//FT06F001 DD SYSOUT=Q
//FT21F001 DD DSN=TDB.STATS.TAFS,DISP=OLD
//FT22F001 DD DSN=TDB.STATS.METARS,DISP=OLD
//FT23F001 DD DSN=TDB.STATS.LNDSYN,DISP=OLD
//FT24F001 DD DSN=TDB.STATS.SHPSYN,DISP=OLD
//FT25F001 DD DSN=TDB.STATS.SREW,DISP=OLD
//FT26F001 DD DSN=TDB.STATS.NCM,DISP=OLD
//FT27F001 DD DSN=TDB.STATS.ESAWS,DISP=OLD
//FT28F001 DD DSN=TDB.STATS.PRESTEL,DISP=OLD
//FT29F001 DD DSN=TDB.STATS.TBUS,DISP=OLD
//FT30F001 DD DSN=TDB.STATS.BOGUS,DISP=OLD
//FT31F001 DD DSN=TDB.STATS.ATAFS,DISP=OLD
//FT32F001 DD DSN=TDB.STATS.MOBSYN,DISP=OLD
//FT51F001 DD DSN=TDB.TAFS,DISP=SHR
//FT52F001 DD DSN=TDB.METARS,DISP=SHR
//FT53F001 DD DSN=TDB.LNDSYN,DISP=SHR
//FT54F001 DD DSN=TDB.SHPSYN,DISP=SHR
//FT55F001 DD DSN=TDB.SREW,DISP=SHR
//FT56F001 DD DSN=TDB.NCM,DISP=SHR
//FT57F001 DD DSN=TDB.ESAWS,DISP=SHR
//FT58F001 DD DSN=TDB.PRESTEL,DISP=SHR
//FT59F001 DD DSN=TDB.TBUS,DISP=SHR
//FT60F001 DD DSN=SDB.BOGUS,DISP=SHR    (THERE IS NO "TDB.BOGUS")
//FT61F001 DD DSN=TDB.ATAFS,DISP=SHR
//FT62F001 DD DSN=TDB.MOBSYN,DISP=SHR
//
