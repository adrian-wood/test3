//SEND EXEC PGM=FTP,REGION=4M,PARM='(EXIT'
//OUTPUT DD SYSOUT=*
//SYSPRINT DD SYSOUT=*
//INPUT DD *
dart.metoffice.gov.uk
cd /metdb/txt
ascii
put 'MDB.BADC.WINPRO.OUT' mdb.winpro.tmp
rename mdb.winpro.tmp ukmo-metdb_winpro_%CYYYYMM.%CDD.%CHHMM..txt
quit
/*
