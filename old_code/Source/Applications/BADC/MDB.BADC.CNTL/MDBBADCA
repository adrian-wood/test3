//FT05F001 DD *
RERUN
//FT06F001 DD SYSOUT=Q
//CEEOPTS  DD DSN=MDB.BADC.DATA(ENVMAIN),DISP=SHR
//SERVER   DD DISP=(,PASS),DSN=&&TEMP1,LRECL=80,RECFM=FB,BLKSIZE=800
//FT10F001 DD DSN=MDB.BADC.WINPRO.OUT,DISP=(NEW,CATLG),
//    STORCLAS=SCDATPRK,MGMTCLAS=MCSNC4,SPACE=(CYL,(40,1)),
//    DCB=(RECFM=FB,LRECL=310,BLKSIZE=27900)
//FT21F001 DD DSN=MDB.BADC.DATA(WINPRO),DISP=SHR
//FT22F001 DD DSN=MDB.BADC.WINPRO.CONTROL,DISP=SHR
//*
//MAIN EXEC PGM=RETBADC,REGION=8M
//STEPLIB DD DSN=MDB.BADC.LOAD,DISP=SHR
//FT05F001 DD *
NORMAL
//FT06F001 DD SYSOUT=Q
